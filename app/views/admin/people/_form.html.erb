<div id="errors"> 
	<ul>
	<% @person.errors.full_messages.each do |msg| %>
	    <li><%= msg %></li>
	  <% end %>
 </ul>
</div>
<%= semantic_form_for [:admin, @person] do |f| %>
  <%= f.inputs :first_name, :last_name, :email, :image, :as => :file %> 
 <%= f.actions(:submit) %>
<% end %> 


     
      
 
 
